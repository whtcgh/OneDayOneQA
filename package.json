{
    "name": "onedayone-qa",
    "displayName": "每日一题",
    "description": "日进一步",
    "version": "0.0.1",
    "publisher": "OneDayOneQA",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:OneDayOneQA.helloWorld",
        "onCommand:extension.helloVscode"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "OneDayOneQA.helloWorld",
                "title": "Hello World"
            },{
                "command": "extension.helloVscode",
                "title": "Hello VScode"
            }
        ],
        "keybindings": [
            {
                "command": "OneDayOneQA.helloWorld",
                "key": "f7",
                "mac": "f7",
                "when": "editorTextFocus"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorFocus",
                    "command": "extension.helloVscode",
                    "group": "navigation"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "beautifulGirl",
                    "title": "每日一题",
                    "icon": "./imgages/icon.png"
                }
            ]
        },
        "views": {
            "beautifulGirl": [
                {
                    "id": "beautifulGirl1",
                    "name": "前端"
                },
                {
                    "id": "beautifulGirl2",
                    "name": "PHP"
                },
                {
                    "id": "beautifulGirl3",
                    "name": "SQL"
                },
                {
                    "id": "beautifulGirl4",
                    "name": "Python"
                }
            ]
        },
        "iconThemes": [
            {
                "id": "testIconTheme",
                "label": "图标",
                "path": "./imgages/icon.jpg"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.56.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "eslint": "^7.27.0",
        "glob": "^7.1.7",
        "mocha": "^8.4.0",
        "typescript": "^4.3.2",
        "vscode-test": "^1.5.2"
    }
}
