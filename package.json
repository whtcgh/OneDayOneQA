{
    "name": "onedayone-qa",
    "displayName": "OneDayOneQA",
    "description": "每日一题",
    "version": "0.0.1",
    "publisher": "OneDayOneQA",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:OneDayOneQA.helloWorld",
        "onCommand:extension.helloVscode"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "OneDayOneQA.helloWorld",
                "title": "Hello World"
            },{
                "command": "extension.helloVscode",
                "title": "Hello VScode"
            }
        ],
		"keybindings": [
			{
				"command": "OneDayOneQA.helloWorld",
				"key": "f7",
				"mac": "f7",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"command": "extension.helloVscode",
					"group": "navigation"
				}
			]
		}
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.56.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "eslint": "^7.27.0",
        "glob": "^7.1.7",
        "mocha": "^8.4.0",
        "typescript": "^4.3.2",
        "vscode-test": "^1.5.2"
    }
}
